{
  "name": "anonchat",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "server:clean": "rimraf ./server/build/",
    "server:build": "npm-run-all server:lint server:format server:clean && tsc",
    "server:start": "node server/src/build/",
    "server:local": "ts-node server/src/index.ts",
    "server:local:watch": "nodemon server/src -e ts,json --exec 'npm run server:local'",
    "server:lint": "eslint server/src",
    "server:format": "npx prettier --write server/src",
    "server:format:check": "npx prettier --check server/src",
    "client:clean": "rimraf ./client/build/",
    "client:build": "npm-run-all client:lint client:format client:clean && tsc",
    "client:start": "node client/src/build/",
    "client:local": "ts-node client/src/index.ts",
    "client:local:watch": "nodemon client/src -e ts,json --exec 'npm run client:local'",
    "client:lint": "eslint client/src",
    "client:format": "npx prettier --write client/src",
    "client:format:check": "npx prettier --check client/src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LionyxML/anonchat.git"
  },
  "keywords": [],
  "author": "",
  "license": "GPL2",
  "bugs": {
    "url": "https://github.com/LionyxML/anonchat/issues"
  },
  "homepage": "https://github.com/LionyxML/anonchat#readme",
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "ws": "^8.13.0",
    "zustand": "^4.3.8"
  }
}
